<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.web.WebView?>

<AnchorPane prefHeight="398" prefWidth="598" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.eapp.Libraries">
    <children>
        <WebView fx:id="webView" layoutX="5.0" layoutY="9.0" prefHeight="246.0" prefWidth="478.0" />
        <Button fx:id="story1" alignment="CENTER" contentDisplay="CENTER" layoutX="161.0" layoutY="153.0" mnemonicParsing="false" onAction="#onStory1ButtonClick" prefHeight="35.0" prefWidth="242.0" text="The Ants &amp; the Grasshopper" textAlignment="CENTER" textFill="#999393">
            <font>
                <Font name="Baskerville Old Face" size="18.0" />
            </font></Button>
        <Button fx:id="story2" alignment="CENTER" contentDisplay="CENTER" layoutX="162.0" layoutY="192.0" mnemonicParsing="false" onAction="#onStory2ButtonClick" prefHeight="35.0" prefWidth="242.0" text="The Ass &amp; the Load of Salt" textAlignment="CENTER" textFill="#999393">
            <font>
                <Font name="Baskerville Old Face" size="18.0" />
            </font></Button>
        <Button fx:id="story3" alignment="CENTER" contentDisplay="CENTER" layoutX="162.0" layoutY="231.0" mnemonicParsing="false" onAction="#onStory3ButtonClick" prefHeight="35.0" prefWidth="242.0" text="The Mice &amp; the Weasels" textFill="#999393">
            <font>
                <Font name="Baskerville Old Face" size="18.0" />
            </font></Button>
        <Button fx:id="story4" layoutX="162.0" layoutY="270.0" mnemonicParsing="false" onAction="#onStory4ButtonClick" prefHeight="36.0" prefWidth="242.0" text="The Monkey &amp; the Dolphin" textFill="#999393">
            <font>
                <Font name="Baskerville Old Face" size="18.0" />
            </font>
        </Button>
        <Button fx:id="story5" alignment="CENTER" contentDisplay="CENTER" layoutX="160.0" layoutY="310.0" mnemonicParsing="false" onAction="#onStory5ButtonClick" prefHeight="35.0" prefWidth="242.0" text="The Peacock &amp; the Crane" textAlignment="CENTER" textFill="#999393">
            <font>
                <Font name="Baskerville Old Face" size="18.0" />
            </font>
        </Button>
        <ButtonBar fx:id="helperBar" layoutX="135.0" layoutY="350.0" prefHeight="40.0" prefWidth="270.0">
            <buttons>
                <Button fx:id="playButton" mnemonicParsing="false" text="Play" />
                <Button fx:id="pauseButton" mnemonicParsing="false" text="Pause" />
                <Button fx:id="stopButton" mnemonicParsing="false" text="Return" />
            </buttons>
        </ButtonBar>
        <Button fx:id="mainPage" layoutX="-24.0" layoutY="-15.0" mnemonicParsing="false" text="&lt;&lt; Back" textFill="#999393">
            <font>
                <Font name="Baskerville Old Face" size="16.0" />
            </font></Button>
        <ImageView fx:id="logo" fitHeight="106.0" fitWidth="89.0" layoutX="235.0" layoutY="9.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../../../Library/logo.png" />
            </image>
        </ImageView>
        <Label fx:id="listView" layoutX="127.0" layoutY="100.0" prefHeight="42.0" prefWidth="307.0" text="List of Tepper's Must-reads" textFill="#666060">
            <font>
                <Font name="Baskerville Old Face" size="28.0" />
            </font>
        </Label>
        <ImageView fx:id="logoFooter" fitHeight="35.0" fitWidth="303.0" layoutX="139.0" layoutY="368.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../../../Library/vines.png" />
            </image>
        </ImageView>
    </children>
</AnchorPane>
