<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.web.WebView?>

<AnchorPane prefHeight="469" prefWidth="672" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.eapp.Libraries">
    <children>
        <Button fx:id="story1" layoutX="88.0" layoutY="14.0" mnemonicParsing="false" onAction="#onStory1ButtonClick" text="Story1" />
        <Button fx:id="story2" layoutX="27.0" layoutY="14.0" mnemonicParsing="false" onAction="#onStory2ButtonClick" text="Story2" />
        <WebView fx:id="webView" layoutX="18.0" layoutY="42.0" prefHeight="334.0" prefWidth="563.0" />
      <ButtonBar fx:id="helperBar" layoutX="150.0" layoutY="336.0" prefHeight="40.0" prefWidth="270.0">
        <buttons>
            <Button fx:id="playButton" mnemonicParsing="false" text="Play" />
            <Button fx:id="pauseButton" mnemonicParsing="false" text="Pause" />
            <Button fx:id="stopButton" mnemonicParsing="false" text="Stop" />
        </buttons>
      </ButtonBar>
    </children>
</AnchorPane>
